{
  "name": "playwright_jenkins",
  "version": "1.0.0",
  "description": "E2E testing with Playwright",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "login:line": "playwright test --grep=@login --reporter=line",
    "login:html": "playwright test --grep=@login --reporter=html",
    "forgot_password:line":"playwright test --grep=@forgot_password --reporter=line",
    "forgot_password:html":"playwright test --grep=@forgot_password --reporter=html",
    "jenkins": "java -jar jenkins.war -httpPort=8080 --enable-future-java"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuocBaoNguyen11295/playwright_jenkins.git"
  },
  "keywords": [
    "ts",
    "js",
    "playwright",
    "e2e",
    "jenkins"
  ],
  "author": "Quoc Bao Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/QuocBaoNguyen11295/playwright_jenkins/issues"
  },
  "homepage": "https://github.com/QuocBaoNguyen11295/playwright_jenkins#readme",
  "dependencies": {
    "@playwright/test": "^1.27.1",
    "dotenv": "^16.0.3",
    "prettier": "^2.8.4"
  }
}
